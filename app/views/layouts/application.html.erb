<!DOCTYPE html>
<html>
  <head>
    <title>IrbApp</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'main', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.dropotron.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.scrollgress.min', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
		<div id="page-wrapper">
    <%= render 'layouts/header' %><!--style for Header is wriiten in file _header.html.erb-->
    <%= yield %>
    <%= render 'layouts/footer' %><!--style for Header is wriiten in file _footer.html.erb-->
    </div>
  </body>
</html>
